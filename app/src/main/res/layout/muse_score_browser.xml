<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.teammatch.yhxle.musica.LearnActivity">
    <Button
        android:id="@+id/browser"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="90dp"
        android:layout_marginStart="5dp"
        android:text="Start Conversion" />

    <Button
        android:id="@+id/stop2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="90dp"
        android:layout_marginStart="150dp"
        android:text="STOP" />

    <Button
        android:id="@+id/playConversion"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="90dp"
        android:layout_marginStart="305dp"
        android:text="Select Midi" />


    <FrameLayout
        android:gravity="center_horizontal"
        android:id="@+id/staff_layout2"
        android:paddingTop="40dp"
        android:paddingLeft="10.0dip"
        android:paddingRight="10.0dip"
        android:layout_width="fill_parent"
        android:layout_height="680dp">
        <TextView
            android:id="@+id/pitch"
            android:layout_marginTop="200dp"
            android:layout_gravity="center_horizontal"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="@string/image_conversion_instruction" />

        <WebView
            android:id="@+id/conversionImage"
            android:layout_marginTop="100dp"
            android:layout_gravity="center_horizontal"
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            android:textSize="20dp" />
    </FrameLayout>


</android.support.design.widget.CoordinatorLayout>
